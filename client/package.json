{
	"name": "mfehostdemo",
	"version": "1.0.0",
	"description": "Host app for the Dev BG MFE demo",
	"main": "index.tsx",
	"author": "Alexander Cheshmedzhiev",
	"license": "MIT",
	"devDependencies": {
		"@babel/core": "7.25.2",
		"@babel/plugin-transform-dynamic-import": "^7.24.7",
		"@babel/preset-env": "7.25.4",
		"@babel/preset-react": "7.24.7",
		"@babel/preset-typescript": "7.24.7",
		"@types/lodash": "4.17.7",
		"@types/react": "18.3.5",
		"@types/react-dom": "18.3.0",
		"@typescript-eslint/eslint-plugin": "6.21.0",
		"@typescript-eslint/parser": "6.21.0",
		"@webpack-cli/generators": "3.0.7",
		"autoprefixer": "10.4.20",
		"babel-loader": "9.1.3",
		"body-parser": "1.20.3",
		"css-loader": "7.1.2",
		"cypress": "13.14.2",
		"eslint": "8.57.0",
		"eslint-plugin-react": "7.36.1",
		"express": "4.21.0",
		"html-webpack-plugin": "5.6.0",
		"mini-css-extract-plugin": "2.9.1",
		"nodemon": "^3.0.1",
		"npm-run-all2": "6.2.3",
		"postcss": "8.4.45",
		"postcss-loader": "8.1.1",
		"prettier": "3.3.3",
		"sass": "1.78.0",
		"sass-loader": "16.0.1",
		"style-loader": "4.0.0",
		"tailwindcss": "^3.4.11",
		"typescript": "5.6.2",
		"wait-on": "8.0.1",
		"webpack": "5.94.0",
		"webpack-cli": "5.1.4",
		"webpack-dev-server": "4.15.2"
	},
	"scripts": {
		"start:client": "webpack serve --node-env=development",
		"wait:client": "wait-on http://localhost:9000",
		"start:client:proxy": "run-s wait:server:proxy start:client",
		"wait:server:proxy": "wait-on http://localhost:9001/health-check",
		"start:mock": "run-p start:server:mock start:client:proxy",
		"start:server:mock": "nodemon --watch devServer ./devServer/server.js",
		"test:component:run": "yarn cypress run --component",
		"test:component:open": "yarn cypress open --component",
		"test:integration:run": "yarn cypress run --e2e",
		"test:integration:open": "yarn cypress open --e2e",
		"build": "webpack --node-env=production --progress",
		"lint": "eslint --ext js,ts,tsx src",
		"tsc": "npx tsc --noEmit",
		"preci": "rm -rf node_modules",
		"ci": "yarn install --frozen-lockfile"
	},
	"engineStrict": true,
	"engines": {
		"node": "20.x",
		"npm": "10.x"
	},
	"dependencies": {
		"i18next": "23.15.1",
		"lodash": "4.17.21",
		"react": "18.3.1",
		"react-dom": "18.3.1",
		"react-error-boundary": "4.0.13",
		"react-i18next": "15.0.1",
		"react-router-dom": "6.26.2"
	}
}
